{
    "name": "cakephp/cakephp-org",
    "description": "The CakePHP framework website",
    "type": "project",
    "keywords": ["cakephp", "framework"],
    "homepage": "http://cakephp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "CakePHP Community",
            "homepage": "https://github.com/cakephp/cakephp/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "3.0.*-dev",
        "cakephp/csfnavbar": "3.0.x-dev",
        "cakephp/migrations": "1.0.1",
        "cakephp/plugin-installer": "*",
        "fguillot/picofeed": "0.1.2",
        "josegonzalez/dotenv": "dev-master",
        "markstory/asset_compress": "1.0.0",
        "mnsami/composer-custom-directory-installer": "1.0.*",
        "mobiledetect/mobiledetectlib": "2.*",
        "predominant/deploy": "*",
        "tedivm/jshrink": "*",
        "bower-asset/anythingslider": "1.9.4",
        "bower-asset/owl-carousel": "dev-master",
        "bower-asset/bootstrap": "3.3.4",
        "bower-asset/google-code-prettify": "1.0.4",
        "bower-asset/modernizr": "dev-master"
    },
    "require-dev": {
        "cakephp/bake": "1.0.1",
        "cakephp/cakephp-codesniffer": "dev-master",
        "cakephp/debug_kit": "3.0.1",
        "phpunit/phpunit": "4.*",
        "psy/psysh": "@stable"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/josegonzalez/asset_compress"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cakephp/csfnavbar"
        },
        {
            "type": "package",
            "package": {
                "name": "predominant/deploy",
                "type": "library",
                "version": "1.0",
                "dist": {
                    "url": "https://github.com/predominant/deploy/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "extra": {
        "installer-paths":{
          "./deploy/": ["predominant/deploy"]
        },
        "asset-repositories": {
            "bower-asset/owl-carousel": {
                "type": "bower-vcs",
                "url": "https://github.com/OwlFonk/OwlCarousel.git"
            },
            "bower-asset/modernizr": {
                "type": "bower-vcs",
                "url": "https://github.com/components/modernizr.git"
            }
        }
    }
}
